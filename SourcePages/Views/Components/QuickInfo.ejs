<div class="quickInfoHolder delayRevealChildren">
	<% if (QuickInfoData.TimeSpent) { %>
		<!-- time spent -->
		<div class="quickInfo">
			<i class="material-icons">schedule</i>
			<h5><%= QuickInfoData.TimeSpent %></h5>
		</div>
	<% }%>
	<% if (QuickInfoData.Date) { %>
		<!-- time spent -->
		<div class="quickInfo">
			<i class="material-icons">date_range</i>
			<h5><%= QuickInfoData.Date %></h5>
		</div>
	<% }%>
	<% if (QuickInfoData.NumPeople) { %>
		<!-- number of people -->
		<% if (QuickInfoData.NumPeople == 1) { %>
			<div class="quickInfo">
				<i class="material-icons">person</i>
				<h5>Solo</h5>
			</div>
		<% }else if (QuickInfoData.NumPeople == 2){ %>
			<div class="quickInfo">
				<i class="material-icons">people</i>
				<h5>2</h5>
			</div>
		<% }else { %>
			<div class="quickInfo">
				<i class="material-icons">groups</i>
				<h5><%= QuickInfoData.NumPeople %></h5>
			</div>
		<% }%>
	<% }%>
	<% if (QuickInfoData.Awards) { %>
		<!-- number of people -->
		<% if (QuickInfoData.Awards == 1) { %>
			<div class="quickInfo">
				<i class="material-icons">workspace_premium</i>
				<h5>Award Winning</h5>
			</div>
		<% }else if (QuickInfoData.Awards > 1){ %>
			<div class="quickInfo">
				<i class="material-icons">workspace_premium</i>
				<h5>Multi Award Winning</h5>
			</div>
		<% }else { %>
			<div class="quickInfo">
				<i class="material-icons">workspace_premium</i>
				<h5><%= QuickInfoData.Awards %></h5>
			</div>
		<% }%>
	<% }%>
	<% if (QuickInfoData.Grade) { %>
		<!-- time spent -->
		<div class="quickInfo">
			<i class="material-icons">grade</i>
			<h5><%= QuickInfoData.Grade %></h5>
		</div>
	<% }%>
</div>
<% if (QuickInfoData.Links) { %>

	<div class="quickLinkHolder delayRevealChildren">

		<!-- where to play/see it -->
		<% if (QuickInfoData.Links.GithubLink) { %>
			<form action="<%= QuickInfoData.Links.GithubLink %>" target="_blank" class="quickInfo sheen">
				<button>
					<%- include('Picture', {ImgData: {
						Path:PageData.PathToRoot + "Public/Assets/Icons/github",
						ClassList: "icon greyScale",
						Alt: "Github Logo",
						IsSvg: true
					}}); %>
					<h5>Code</h5>
				</button>
			</form>
		<% }%>
		<% if (QuickInfoData.Links.ItchLink) { %>
			<form action="<%= QuickInfoData.Links.ItchLink %>" target="_blank" class="quickInfo sheen">
				<button>
					<%- include('Picture', {ImgData: {
						Path:PageData.PathToRoot + "Public/Assets/Icons/itch-dot-io",
						ClassList: "icon greyScale",
						Alt: "Itch Logo",
						IsSvg: true
					}}); %>
					<h5>Play</h5>
				</button>
			</form>
		<% }%>
		<% if (QuickInfoData.Links.WebsiteLink) { %>
			<form action="<%= QuickInfoData.Links.WebsiteLink %>" target="_blank" class="quickInfo sheen">
				<button>
					<%- include('Picture', {ImgData: {
						Path:PageData.PathToRoot + "Public/Assets/Icons/website",
						ClassList: "icon greyScale",
						Alt: "",
						IsSvg: true
					}}); %>
					<h5>Website</h5>
				</button>
			</form>
		<% }%>
		<% if (QuickInfoData.Links.InstagramLink) { %>
			<form action="<%= QuickInfoData.Links.InstagramLink %>" target="_blank" class="quickInfo sheen">
				<button>
					<%- include('Picture', {ImgData: {
						Path:PageData.PathToRoot + "Public/Assets/Icons/Instagram",
						ClassList: "icon greyScale",
						Alt: "Instagram Logo",
						IsSvg: true
					}}); %>
					<h5>Instagram</h5>
				</button>
			</form>
		<% }%>
		<% if (QuickInfoData.Links.GooglePlayLink) { %>
			<a href="<%= QuickInfoData.Links.GooglePlayLink %>" target="_blank" class="quickInfo sheen noUnderLine">
				<%- include('Picture', {ImgData: {
					Path:PageData.PathToRoot + "Public/Assets/Icons/GooglePlayStore",
					ClassList: "icon greyScale",
					Alt: "Google Play Store Logo",
					IsSvg: true
				}}); %>
				<h5>Google Play</h5>
			</a>
		<% }%>
		<% if (QuickInfoData.Links.AppleArcadeLink) { %>
			<form action="<%= QuickInfoData.Links.AppleLink %>" target="_blank" class="quickInfo sheen">
				<button>
					<%- include('Picture', {ImgData: {
						Path:PageData.PathToRoot + "Public/Assets/Icons/Apple",
						ClassList: "icon greyScale",
						Alt: "Apple Logo",
						IsSvg: true
					}}); %>
					<h5>Apple Arcade</h5>
				</button>
			</form>
		<% }%>
		<% if (QuickInfoData.Links.AppStoreLink) { %>
			<form action="<%= QuickInfoData.Links.AppStoreLink %>" target="_blank" class="quickInfo sheen">
				<button>
					<%- include('Picture', {ImgData: {
						Path:PageData.PathToRoot + "Public/Assets/Icons/Apple",
						ClassList: "icon greyScale",
						Alt: "Apple Logo",
						IsSvg: true
					}}); %>
					<h5>App Store</h5>
				</button>
			</form>
		<% }%>
	</div>
<% }%>
