		PageData: PageData,
		HeadData: {
			Title: "Louie J-S: EPQ",
			Description: "Louie Jones-Strong's portfolio. My college EPQ dissertation on multi-technique supervised machine learning.",
			Url: "https://louie-jones-strong.github.io/EPQProject.html",
			Thumbnail: "https://louie-jones-strong.github.io/Assets/EPQ/thumbnail.png",
