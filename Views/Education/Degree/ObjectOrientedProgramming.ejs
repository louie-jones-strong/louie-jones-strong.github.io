				}); %>

				<hr class="gap">
				<h2 class="center reveal fromBottom">Final Project</h2>
				<%- include(PageData.PathToRoot+'Components/QuickInfo', {
					QuickInfoData: {
						TimeSpent: "Month",
						NumPeople: 1,
						Links: {
							<!-- GithubLink: "https://github.com/louie-jones-strong/Uni-year-2-term-1/tree/master/Object-Oriented%20Programming/Juce" -->
						}
					},
				}); %>

			</div>

			<!-- Shared common footer -->
			<%- include(PageData.PathToRoot+'Components/Footer'); %>
