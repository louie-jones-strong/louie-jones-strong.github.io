
<% if (!VideoData.LoadingType) {
	VideoData.LoadingType = "lazy"
}%>

<% if (VideoData.Autoplay) {
	VideoData.Muted = true
}%>

<video
<% if (VideoData.Controls) { %> controls <% }%>
<% if (VideoData.Autoplay) { %> autoplay <% }%>
<% if (VideoData.Muted) { %> muted <% }%>
<% if (VideoData.Loop) { %> loop <% }%>

loading=<%= VideoData.LoadingType %>
<% if (VideoData.ClassList) { %> class="<%= VideoData.ClassList %>" <% }%>
>



<%
let videoConfig = PageData.SiteConfig.AssetConfig.VideoConfig;
let outputFormats = videoConfig.OutputFormats;
let resGroups = videoConfig.HorizontalResolutionsGroups;
for (const outputFormat of outputFormats)
{%>
	<source src="
	<% for (const resGroup of resGroups)
	{%>
		<%= PageData.PathToRoot %><%= VideoData.Path %>_<%= resGroup %>.<%= outputFormat %>
	<%}%>
	" type="video/<%= outputFormat %>"
	<% if (VideoData.Sizes) { %> sizes="<%= VideoData.Sizes %>" <% }%>>
<%}%>


I'm sorry, your browser doesn't support HTML5 video.
</video>


