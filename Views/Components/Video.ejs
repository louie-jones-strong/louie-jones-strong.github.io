
<% if (!VideoData.LoadingType) {
	VideoData.LoadingType = "lazy"
}%>

<% if (VideoData.Autoplay) {
	VideoData.Muted = true
}%>

<video
<% if (VideoData.Controls) { %> controls <% }%>
<% if (VideoData.Autoplay) { %> autoplay <% }%>
<% if (VideoData.Muted) { %> muted <% }%>
<% if (VideoData.Loop) { %> loop <% }%>

loading=<%= VideoData.LoadingType %>
<% if (VideoData.ClassList) { %> class="<%= VideoData.ClassList %>" <% }%>
>

<source src="<%= PageData.PathToRoot %><%= VideoData.Path %>_128.webm" type="video/webm">
<source src="<%= PageData.PathToRoot %><%= VideoData.Path %>_128.mp4" type="video/mp4">
I'm sorry, your browser doesn't support HTML5 video.
</video>


