
<% if (!ImgData.LoadingType) {
	ImgData.LoadingType = "lazy"
}%>

<% if (ImgData.IsSvg) { %>

<img src="<%= PageData.PathToRoot %><%= ImgData.Path %>.svg" alt="<%= ImgData.Alt %>" loading=<%= ImgData.LoadingType %>
	<% if (ImgData.ClassList) { %> class="<%= ImgData.ClassList %>" <% }%>
	<% if (ImgData.Style) { %> style="<%= ImgData.Style %>" <% }%>
>

<% }else{%>

<picture
	<% if (ImgData.ClassList) { %> class="<%= ImgData.ClassList %>" <% }%>
	<% if (ImgData.Style) { %> style="<%= ImgData.Style %>" <% }%>
	>


	<source srcset="
		<%= PageData.PathToRoot %><%= ImgData.Path %>_128.webp 128w,
		<%= PageData.PathToRoot %><%= ImgData.Path %>_256.webp 256w,
		<%= PageData.PathToRoot %><%= ImgData.Path %>_512.webp 512w,
		<%= PageData.PathToRoot %><%= ImgData.Path %>_1024.webp 1024w"
	type="image/webp"
	<% if (ImgData.Sizes) { %> sizes="<%= ImgData.Sizes %>" <% }%>>

	<source srcset="
		<%= PageData.PathToRoot %><%= ImgData.Path %>_128.png 128w,
		<%= PageData.PathToRoot %><%= ImgData.Path %>_256.png 256w,
		<%= PageData.PathToRoot %><%= ImgData.Path %>_512.png 512w,
		<%= PageData.PathToRoot %><%= ImgData.Path %>_1024.png 1024w"
	<% if (ImgData.Sizes) { %> sizes="<%= ImgData.Sizes %>" <% }%>>

	<img src="<%= PageData.PathToRoot %><%= ImgData.Path %>_1024.png" alt="<%= ImgData.Alt %>" loading=<%= ImgData.LoadingType %>
	<% if (ImgData.Sizes) { %> sizes="<%= ImgData.Sizes %>" <% }%>>
</picture>

<% }%>