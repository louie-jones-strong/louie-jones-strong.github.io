							<source src="Public/Assets/EPQ/videos/middle.mp4" type="video/mp4">
							I'm sorry, your browser doesn't support HTML5 video.
						</video>
						<div class="slide-caption">
							<h4>Training mid point</h4>
						</div>
					</div>
					<div class="slide">
						<video autoplay muted loop>
							<source src="Public/Assets/EPQ/videos/best.mp4" type="video/mp4">
							I'm sorry, your browser doesn't support HTML5 video.
						</video>
						<div class="slide-caption">
							<h4>End of Training</h4>
						</div>
					</div>
					<span class="slide-dot"></span>
					<span class="slide-dot"></span>
					<span class="slide-dot"></span>
				</div>
				<script src="Public/js/Carousel.js"></script>
			</div>
			<div class="ZipperCard shaded glass reveal fromRight">
				<%- include('Components/Picture', {ImgData: {
					Path:"Public/Assets/EPQ/Graphs/TimeToLearn",
