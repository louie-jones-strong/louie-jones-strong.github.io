	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "clean and compress",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\src\\BuildSite.js",
			"args": ["clean", "compress"]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "compress",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\src\\BuildSite.js",
			"args": ["compress", "onlyNew"]
		}
	]
}